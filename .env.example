# OpenAI API Key for LLM integration
OPENAI_API_KEY=sk-your-openai-api-key-here

# PostgreSQL Database Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-secure-password-here
POSTGRES_DB=aggroso

# Database URL
# For Docker: use db:5432 (internal container network)
# For local dev: use localhost:5433 (mapped port)
DATABASE_URL=postgresql://postgres:your-secure-password-here@db:5432/aggroso

# Node Environment
NODE_ENV=development
